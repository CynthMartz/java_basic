	numeros[0]= 3;
	numeros[1]= 2;
	numeros[2]= 7;
	numeros[3]= 6;
	
	// 
	int[] arreglo4 = new int[100];
		for (int i = 0; i < arreglo4.length; i = i + 1) {
		arreglo4[i] = i+1;
	}
	
	System.out.println(Arrays.toString(arreglo4));
	
	//solucion 1
			int[] arreInt = new int [100];
			for (int i = 0; i < arreInt.length; i++) {
				arreInt[i]=i;
				System.out.println("Elemento :"+(i+1));
